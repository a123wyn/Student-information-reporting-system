<view class="body"><view class="collect">已统计<label class="_span">{{stat}}</label>人，缺<label class="_span">{{lack}}</label>人<view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="select" bindtap="__e">当前所选时间:<label class="_span">{{time}}</label></view></view><block wx:for="{{arr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['list',(index%2==0)?'listBgc':'',(index==0)?'listOne':'',(index==stat-1)?'listFinally':'']}}" style="{{'color:'+(item.temperature>37||item.contact_like_virus==1||item.Suspected_symptoms==1?'#f00':'#000')+';'}}"><view class="top"><view class="id">{{"学号:"+item.stu_num}}</view><view class="name">{{"姓名:"+item.stu_name}}</view><view class="name">健康状态:<view style="{{'color:'+(item.health=='良好'?'#1aad19':'#f00')+';'}}">{{item.health}}</view></view></view><view class="top">{{''+(item.current_pos==2?'在湖北':item.current_pos==1?'在外地':'在本地')+", 今日体温:"+item.temperature+",\n\t\t\t\t"+(item.contact_virus==0?'未接触湖北地区人员':'接触过湖北地区人员')+", "+(item.have_symptom==0?'无疑似症状':'出现过疑似症状')+''}}</view></view></block><block wx:if="{{selectTime==true}}"><view class="time"><view data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" class="masking" bindtap="__e"></view><uni-calendar class="calendars" vue-id="2e637200-1" insert="{{true}}" lunar="{{true}}" start-date="2019-3-2" end-date="2019-5-20" data-event-opts="{{[['^change',[['change']]]]}}" bind:change="__e" bind:__l="__l"></uni-calendar></view></block><block wx:if="{{arr.length==0}}"><view class="null">该日期未登记健康状态</view></block></view>